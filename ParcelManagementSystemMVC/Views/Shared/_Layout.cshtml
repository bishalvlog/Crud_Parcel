<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="Responsive Admin &amp; Dashboard Template based on Bootstrap 5">
    <meta name="author" content="AdminKit">
    <meta name="keywords" content="adminkit, bootstrap, bootstrap 5, admin, dashboard, template, responsive, css, sass, html, theme, front-end, ui kit, web">

    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="shortcut icon" href="~/adminkit/img/icons/icon-48x48.png" />

    <link rel="canonical" href="https://demo-basic.adminkit.io/" />
	<link rel="stylesheet" href="~/css/site.css" />

	<title>@ViewData["Title"] -Admin-  ParcelManagementSystemMVC</title>
	<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
	<link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
    <link href="~/adminkit/css/app.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@300;400;600&display=swap" rel="stylesheet">
	<script src="https://kit.fontawesome.com/fda09458da.js" crossorigin="anonymous"></script>
</head>
<body>
	<div class="wrapper">
		<nav id="sidebar" class="sidebar js-sidebar">
			<div class="sidebar-content js-simplebar">
				<a class="sidebar-brand" asp-controller="Home" asp-action="Index">
					<span class="align-middle">AdminPage</span>
				</a>

				<ul class="sidebar-nav">
					<li class="sidebar-header">
						DashBoard
					</li>

					<li class="sidebar-item active">
						<a class="sidebar-link" asp-controller="Parcel" asp-action="DashBoard">
							<i class="align-middle" data-feather="sliders"></i> <span class="align-middle">Dashboard</span>
						</a>
					</li>
					<li class="sidebar-header">
						Parcel
					</li>
					<li class="sidebar-item">
						<a class="sidebar-link" asp-controller="Parcel" asp-action="AddParcel" >
							<i class="align-middle" data-feather="plus"></i> <span class="align-middle">Add New Parcel</span>
						</a>
					</li>

					<li class="sidebar-item">
						<a class="sidebar-link" asp-controller="Parcel" asp-action="ParcelList">
							<i class="align-middle" data-feather="package"></i> <span class="align-middle">Parcel List</span>
						</a>
					</li>

					<li class="sidebar-item">
						<a class="sidebar-link" asp-controller="Parcel" asp-action="TrackParcel">
							<i class="align-middle" data-feather="activity"></i> <span class="align-middle">Track Parcel</span>
						</a>
					</li>
					<li class="sidebar-header">
						User
					</li>
					<li class="sidebar-item">
						<a class="sidebar-link" asp-controller="User" asp-action="AddUser">
							<i class="align-middle" data-feather="user-plus"></i> <span class="align-middle">Add New User</span>
						</a>
					</li>
					<li class="sidebar-item">
						<a class="sidebar-link" asp-controller="User" asp-action="UserList">
							<i class="align-middle" data-feather="users"></i> <span class="align-middle">User List</span>
						</a>
					</li>

					<li class="sidebar-header">
						Settings
					</li>

					<li class="sidebar-item" >
						<a class="sidebar-link" asp-controller="Parcel" asp-action="Setting">
							<i class="align-middle" data-feather="settings"></i> <span class="align-middle">Settings</span>
						</a>
					</li>

					
				</ul>

				
			</div>
		</nav>

		<div class="main">
			<nav class="navbar navbar-expand navbar-light navbar-bg">
				<a class="sidebar-toggle js-sidebar-toggle">
					<i class="hamburger align-self-center"></i>
				</a>

				<div class="navbar-collapse collapse">
					<a class="btn dropdown-toggle d-none d-sm-inline-block " data-bs-toggle="dropdown">
						<span class="text-dark">AdminPage</span>
					</a>
					<div class="dropdown-menu">
						<a class="dropdown-item" asp-controller="Parcel" asp-action="Dashboard"> AdminPage</a>
						<a class="dropdown-item" asp-controller="AdminUser" asp-action="Dashboard">Master AdminPage</a>

					</div>
					<ul class="navbar-nav navbar-align">
						<li class="navbar-item"><a class="btn btn-primary m-1" asp-controller="Parcel" asp-action="AddParcel">New Parcel</a></li>
						<li class="navbar-item"><a class="btn btn-secondary m-1" asp-controller="Parcel" asp-action="TrackParcel">Track Parcel</a></li>
						
						
						<li class="nav-item dropdown">
							<a class="nav-icon dropdown-toggle d-inline-block d-sm-none" href="#" data-bs-toggle="dropdown">
								<i class="align-middle" data-feather="more-vertical"></i>
							</a>

							<a class="nav-link dropdown-toggle d-none d-sm-inline-block" href="#" data-bs-toggle="dropdown">
								<img src="/adminkit/img/avatars/avatar.jpg" class="avatar img-fluid rounded-circle me-1" alt="User" /> <span class="text-dark">UserName</span>
							</a>
							<div class="dropdown-menu dropdown-menu-end">
								<a class="dropdown-item" href="pages-profile.html"><i class="align-middle me-1" data-feather="user"></i> Profile</a>
								<a class="dropdown-item" href="#"><i class="align-middle me-1" data-feather="pie-chart"></i> Analytics</a>
								<div class="dropdown-divider"></div>
								<a class="dropdown-item" href="index.html"><i class="align-middle me-1" data-feather="settings"></i> Settings & Privacy</a>
								<a class="dropdown-item" href="#"><i class="align-middle me-1" data-feather="help-circle"></i> Help Center</a>
								<div class="dropdown-divider"></div>
								<a class="dropdown-item" href="#">Log out</a>
							</div>
						</li>
					</ul>
				</div>
			</nav>
			<div class="container-fluid m-3">
			<main>@RenderBody()</main>
			</div>
			
		</div>
		
	</div>
	<footer class="border-top footer text-muted">
		<div class="container">
			&copy; 2023 - ParcelManagementSystemMVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
		</div>
	</footer>


<script src="~/lib/jquery/dist/jquery.min.js"></script>
@*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
*@<script src="~/adminkit/js/app.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>


@await RenderSectionAsync("Scripts", false)
</body>
</html>